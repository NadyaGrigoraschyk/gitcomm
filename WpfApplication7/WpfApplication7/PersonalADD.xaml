<Window x:Class="WpfApplication7.PersonalADD"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApplication7"
        mc:Ignorable="d"
        Title="PersonalADD" Height="300" Width="200">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Orientation="Vertical">
            <Label Content="Id"></Label>
            <TextBox x:Name="TId" Text="" Width="130" Height="22"></TextBox>
            <Label Content="Name"></Label>
            <TextBox x:Name="TName" Text="" Width="130" Height="22"></TextBox>
            <Label Content="Surname"></Label>
            <TextBox x:Name="TSurname" Text="" Width="130" Height="22"></TextBox>
            <Label Content="Age"></Label>
            <TextBox x:Name="TAge" Text="" Width="130" Height="22"></TextBox>
            <Label Content="Doljnost"></Label>
            <TextBox x:Name="TDoljnost" Text="" Width="130" Height="22"></TextBox>
            <Button x:Name="Add" Content="ADD" Width="120" Height="22" Margin="10" Click="Add_Click"></Button>
        </StackPanel>
    </Grid>
</Window>
